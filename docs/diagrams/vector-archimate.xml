    <relationship identifier="id-rel-81" xsi:type="Composition" source="id-app-uw-dashboard" target="id-appinterface-signalr"/>

    <!-- Technology deployment -->
    <relationship identifier="id-rel-90" xsi:type="Composition" source="id-node-aks" target="id-node-apipod"/>
    <relationship identifier="id-rel-91" xsi:type="Composition" source="id-node-aks" target="id-node-webpod"/>
    <relationship identifier="id-rel-92" xsi:type="Composition" source="id-node-aks" target="id-node-workerpod"/>

    <relationship identifier="id-rel-100" xsi:type="Assignment" source="id-artifact-apiimage" target="id-node-apipod"/>
