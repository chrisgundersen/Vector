{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ]
  },
  "UseInMemoryDatabase": true,
  "UseMockServices": true,
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=VectorDb;Trusted_Connection=True;MultipleActiveResultSets=true",
    "BlobStorage": "",
    "Redis": ""
  },
  "EmailService": {
    "Provider": "Mock",
    "Graph": {
      "TenantId": "",
      "ClientId": "",
      "ClientSecret": "",
      "ProcessedFolderName": "Processed"
    }
  },
  "MessageBus": {
    "Provider": "InMemory",
    "AzureServiceBus": {
      "ConnectionString": "",
      "EmailIngestionQueue": "email-ingestion",
      "DocumentProcessingQueue": "document-processing"
    }
  },
  "EmailPolling": {
    "IntervalSeconds": 60,
    "MaxEmailsPerPoll": 10,
    "EnableMessageDeduplication": true,
    "MailboxIds": [ "submissions@vector.local" ],
    "DefaultTenantId": "00000000-0000-0000-0000-000000000001"
  },
  "DocumentProcessing": {
    "IntervalSeconds": 30,
    "BatchSize": 5
  },
  "DocumentIntelligence": {
    "Provider": "Mock",
    "Azure": {
      "Endpoint": "",
      "ApiKey": "",
      "ClassifierModelId": "",
      "Acord125ModelId": "",
      "Acord126ModelId": "",
      "Acord130ModelId": "",
      "Acord140ModelId": "",
      "LossRunModelId": "",
      "ExposureScheduleModelId": "",
      "ModelVersion": "1.0"
    }
  }
}
